{
  "name": "create-meh-app",
  "version": "1.14.2",
  "description": "Quickly scaffold a new Node.js project by creating a GitLab repository, setting up linting/formatting, automatic Kubernetes deployment, local dotenv â‡„ Kubernetes secrets syncing, and more.",
  "bin": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --check '**/*.{css,graphql,html,json,md,scss}'",
    "prepublishOnly": "yarn lint && yarn format"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{css,graphql,html,json,md,scss}": [
      "yarn format --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "repository": "github:greenhousecode/create-meh-app",
  "author": "Rick Stevens <rick.stevens@greenhousegroup.com> (http://www.greenhousegroup.com/)",
  "devDependencies": {
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@gitbeaker/node": "^23.5.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.0.0",
    "inquirer": "^7.3.3",
    "promise.allsettled": "^1.0.2",
    "rimraf": "^3.0.2",
    "simple-git": "^2.17.0",
    "yaml": "^1.10.0"
  }
}
